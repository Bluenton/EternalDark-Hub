    key = value
end)

section1:addButton("Submit", function()
    if key == game:HttpGet("https://raw.githubusercontent.com/EternalDarkness-Hub/EternalDarkness-Hub.github.io/main/KeySystem/Key.txt") then
        saveSettings()
        venyx:Notify("Key System", "Correct Key!")
        venyx:toggle()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Bluenton/EternalDark-Hub/main/GameChecker", true))();
    else
        venyx:Notify("Key System", "Wrong Key!")
    end
end)

