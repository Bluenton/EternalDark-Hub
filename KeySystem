local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("EternalDarkness Hub")

local serv = win:Server("Synapse X Chan", "")

local textbs = serv:Channel("Log In")
local supported = serv:Channel("Supported Games")
local info = serv:Channel("Information")
local key = nil

textbs:Textbox("Key", "Type Key", true, function(key)
    if key == game:HttpGet("https://raw.githubusercontent.com/EternalDarkness-Hub/EternalDarkness-Hub.github.io/main/KeySystem/Key.txt") then
        game.CoreGui.Discord:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Bluenton/EternalDark-Hub/main/GameChecker", true))();
    else
        print("Wrong Key!")
    end
end)

textbs:Button("Get Key", function()
    DiscordLib:Notification("Notification", "Copied to clipboard!", "Okay")
    setclipboard("https://eternaldarkness-hub.github.io/KeySystem/index.html")
	toclipboard("https://eternaldarkness-hub.github.io/KeySystem/index.html")
end)

supported:Label("Jailbreak")
supported:Label("Elephant Hotel")
supported:Label("SharkBite")
supported:Label("Business Legends")
supported:Label("Ice Cream Van Simulator")
supported:Label("Tropical Resort Tycoon -- (Being Fixed)")
supported:Label("Murder Mystery 2")
supported:Label("Pet Simulator X")
supported:Label("Hood Modded")
supported:Label("Da Hood")
supported:Label("SCP Roleplay")
supported:Label("Arsenal")
supported:Label("Build A Boat For Treasure")

info:Label("Eternal Darkness hub combines all")
info:Label("best scripts in the internet.")
