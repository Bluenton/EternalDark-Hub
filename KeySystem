    if focusLost then
    msg = value
    end
end)
section4:addButton("Send", function()
    local webhookcheck =
   is_sirhurt_closure and "Sirhurt" or pebc_execute and "ProtoSmasher" or syn and "Synapse X" or
   secure_load and "Sentinel" or
   KRNL_LOADED and "Krnl" or
   SONA_LOADED and "Sona" or
   "shit exploit"

local url =
   "https://discord.com/api/webhooks/913782242319421490/JGHv5i_IDTuWWH0_7BfVqloN7WQ9gd7AxrcNZzmRz9HuJDAPvdbJsx5NF5iqgs5x73T6"
local data = {
   ["embeds"] = {
       {
           ["title"] = "New message from "..game.Players.LocalPlayer.Name.." using "..webhookcheck,
           ["description"] = "Reason: "..msgtype.."\n\n"..msg,
           ["type"] = "rich",
           ["color"] = tonumber(0x7269da),
       }
   }
}
local newdata = game:GetService("HttpService"):JSONEncode(data)

local headers = {
   ["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)
venyx:Notify("Contact", "message sent!")
end)

local filename = "EternalDarkness_Hub_settings.txt"

function loadSettings()
    print(" -- Trying Auto Log In! -- ")
    local HttpService = game:GetService("HttpService")
    if (readfile and isfile and isfile(filename)) then
        key = HttpService:JSONDecode(readfile(filename))
        if key == game:HttpGet("https://raw.githubusercontent.com/EternalDarkness-Hub/EternalDarkness-Hub.github.io/main/KeySystem/Key.txt") then
            print(" -- loging in -- ")
            venyx:toggle()
            venyx:Notify("Key System", "Auto Loged In")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Bluenton/EternalDark-Hub/main/GameChecker", true))();
        else
            venyx:Notify("Key System", "Old key, type the new key!")
            print(" -- Key was wrong! -- ")
        end
    end
end

function saveSettings()
    print(" -- Saving key! -- ")
    local json
    local HttpService = game:GetService("HttpService")
    if (writefile) then
        json = HttpService:JSONEncode(key)
        writefile(filename, json)
    else
        print(" -- Error saving key! -- ")
    end
end

loadSettings()

-- load
venyx:SelectPage(venyx.pages[1], true)

for i,v in pairs(game.Players:GetChildren()) do
    if v.UserId == 479365632 then
        venyx:Notify("System", "Script owner is in the game!\n\n"..v.DisplayName)
    end
end

game.Players.PlayerAdded:Connect(function(v)
    if v.UserId == 479365632 then
        venyx:Notify("System", "Script owner joined the game!\n\n"..v.DisplayName)
    end
end)

game.Players.PlayerRemoving:Connect(function(v)
    if v.UserId == 479365632 then
        venyx:Notify("System", "Script owner left the game!\n\n"..v.DisplayName)
    end
end)